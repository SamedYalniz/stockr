<% if @products.any? %>
  <% @products.each do |product| %>
    <%= product.name %>
    <%= link_to "Edit #{product.name}", edit_product_path(product) %>
    <%= link_to "Delete #{product.name}", product_path(product), method: :delete %>
  <% end %>
<% end %>


<%= link_to 'Add a product', new_product_path %>
